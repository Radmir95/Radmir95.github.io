{
  "name": "react-resume",
  "author": "Vladimir Skok <skok@vova.io>",
  "version": "0.2.0",
  "homepage": "https://iamskok.github.io/react-resume/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "clean-gh-pages": "git push origin :gh-pages",
    "git-commit": "git add -A && git commit -m 'update github pages'",
    "predeploy": "yarn build && yarn clean-gh-pages",
    "deploy": "git subtree push --prefix build origin gh-pages",
    "gh-pages": "yarn git-commit && yarn predeploy && yarn deploy"
  },
  "dependencies": {
    "react": "^16.8.3",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.8.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "react-scripts": "^2.1.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
